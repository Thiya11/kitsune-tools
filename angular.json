{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "libs",
  "projects": {
    "kT": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/k-t",
      "sourceRoot": "apps/k-t/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/k-t",
            "index": "apps/k-t/src/index.html",
            "main": "apps/k-t/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "apps/k-t/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/k-t/src/favicon.ico",
              "apps/k-t/src/assets"
            ],
            "styles": [
              "node_modules/bootstrap/scss/bootstrap.scss",
              "node_modules/bootstrap-icons/font/bootstrap-icons.css",
              "libs/shared/styles/styles.scss",
              "apps/k-t/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "kT:build:production"
            },
            "development": {
              "browserTarget": "kT:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "kT:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "apps/k-t/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/k-t/src/favicon.ico",
              "apps/k-t/src/assets"
            ],
            "styles": [
              "apps/k-t/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "maintanance": {
      "projectType": "library",
      "root": "libs/maintanance",
      "sourceRoot": "libs/maintanance/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "libs/maintanance/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/maintanance/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/maintanance/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "libs/maintanance/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  }
}
